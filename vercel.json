{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "autoAlias": true,
    "autoJobCancelation": true,
    "autoDeployOnPush": true
  },
  "build": {
    "env": {
      "VERCEL_GIT_COMMIT_SHA": "@vercel-git-commit-sha"
    }
  },
  "deployment": {
    "autoDeploy": true
  }
}